{
  "name": "tex-intel-api",
  "version": "1.0.0",
  "description": "Vapi integration API for Tex heavy equipment business",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "postbuild": "mkdir -p dist/db && cp src/db/schema.sql dist/db/schema.sql && cp -R public dist/public",
    "start": "node dist/app.js",
    "dev": "tsx --watch src/app.ts",
    "clean": "rm -rf dist",
    "tunnel": "ngrok http 3000",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "vapi:sync": "tsx scripts/sync-vapi.ts"
  },
  "keywords": [
    "vapi",
    "fastify",
    "voice-ai",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@fastify/cors": "^11.2.0",
    "@fastify/formbody": "^8.0.2",
    "@fastify/static": "^9.0.0",
    "@fastify/swagger": "^9.6.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@types/sqlite3": "^3.1.11",
    "@vapi-ai/server-sdk": "^0.11.0",
    "axios": "^1.13.2",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.2",
    "ngrok": "^5.0.0-beta.2",
    "pino-pretty": "^13.1.3",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "tsx": "^4.21.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@vitest/ui": "^4.0.16",
    "c8": "^10.1.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
